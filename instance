    <title>Document</title>
</head>
<body>
    <script>
    function BODYINDEX(w, h){
        this.weight = w;
        this.height = h/100;
        this.bmi = function(){
            console.log(this.weight/(this.height *this.height));
        }
    }

    
    let people =[new BODYINDEX(60,177),new BODYINDEX(50,190),new BODYINDEX(100,160)];

    for(let i=0 ;i<people.length; i++){
       people[i].bmi();
    }


    </script>
</body>
</html>
